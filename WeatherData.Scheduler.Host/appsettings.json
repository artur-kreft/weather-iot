{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] "
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "",
    "BlobStorageConnection": ""
  },
  "Threads": {
    "SemaphoreCount": 20
  },
  "BlobStorage": {
    "ContainerId": "iotbackend",
    "MetadataFileName": "metadata.csv",
    "HistoryFileName": "historical.zip",
    "HistoryPageSize": 100,
    "CsvDelimiter": ";",
    "CsvFilenameFormat": "yyyy-MM-dd",
    "TimestampFormat": "yyyy-MM-dd'T'HH:mm:ss"
  },
  "Importer": {
    "UpdateRetriesDaysLimit": 7,
    "TryToReenableDeviceDaysPeriod": 7 
  }
}